<html>
    <center>
        <form action="/flight/editimage" method="post" enctype="multipart/form-data">
            <input type="hidden" value="<%=data.fid%>" name="fid">
            <h3><%=data.fname%></h3>
            <h3><%=data.company%></h3>
            <img src="/images/<%=data.logo%>" width="250"><br>
            <b>Upload new image:</b><br><br>
            <input type="file" required name="logo">
            <input type="submit" value="Upload">
        </form>
    </center>
</html>
